{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "sitemap.xml",
      "use": "@vercel/static"
    },
    {
      "src": "robots.txt",
      "use": "@vercel/static"
    },
    {
      "src": "g/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "game/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "*.{png,css,js,json}",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/g/(.*)",
      "dest": "/g/$1"
    },
    {
      "src": "/game/(.*)",
      "dest": "/game/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
